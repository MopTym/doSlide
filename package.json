{
  "name": "do-slide",
  "version": "1.1.4",
  "description": "Fullpage scroll / Section scroll / Slider / No dependency",
  "main": "dist/do-slide.js",
  "scripts": {
    "watch": "webpack -w --config build/webpack.config.js",
    "build": "gulp && webpack --config build/webpack.config.js && webpack -p --config build/webpack.min.config.js",
    "build-test": "webpack --config build/webpack.test.config.js",
    "unit": "karma start build/karma.unit.config.js",
    "test": "npm run unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MopTym/doSlide.git"
  },
  "keywords": [
    "slide",
    "scroll",
    "fullscreen",
    "fullpage",
    "slider",
    "switch"
  ],
  "author": "MopTym <moptym@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MopTym/doSlide/issues"
  },
  "homepage": "https://github.com/MopTym/doSlide",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglifycss": "^1.0.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.3",
    "webpack": "^1.12.9"
  }
}
